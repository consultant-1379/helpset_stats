var chl;
chl = new Array;
var chlCount;
chlCount = new Array;
var activeFolder = "C_";

var fT = 0;
var nextNodeId = 0;
var scrollPx = 0;
var treeStatus = 0;

function generateTree() {
fT=cN("&nbsp;Ericsson Network IQ Security Admin Guide","C0");
h1=aN(fT,cN("About this Document","C1"));
