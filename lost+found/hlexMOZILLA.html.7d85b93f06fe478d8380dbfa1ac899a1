var C9_1_3=
"<div id=\"content\" class=\"body-content\">\n"+
"<h3><span class=\"CHAPNUMBER\">9.1.3 &nbsp; </span><span class=\"CHAPTITLE\"><a name=\"id_l63u\"></a><a name=\"CHAPTER9.1.3\" href='javascript:parent.parent.sC2(\"TOP\")' class=\"CHAPLINK\">Security Exception in Chrome Browser</a></span></h3>\n" +
