var C13_2=
"<div id=\"content\" class=\"body-content\">\n"+
"<h2><span class=\"CHAPNUMBER\">13.2 &nbsp; </span><span class=\"CHAPTITLE\"><a name=\"id_rhx9\"></a><a name=\"CHAPTER13.2\" href='javascript:parent.parent.sC2(\"TOP\")' class=\"CHAPLINK\">Stopping\n" +
