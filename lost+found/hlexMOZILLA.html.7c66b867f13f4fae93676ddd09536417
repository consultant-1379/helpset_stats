var C18_4=
"<div id=\"content\" class=\"body-content\">\n"+
"<h2><span class=\"CHAPNUMBER\">18.4 &nbsp; </span><span class=\"CHAPTITLE\"><a name=\"id_3ygs\"></a><a name=\"CHAPTER18.4\" href='javascript:parent.parent.sC2(\"TOP\")' class=\"CHAPLINK\">Checking License Manager and License\n" +
