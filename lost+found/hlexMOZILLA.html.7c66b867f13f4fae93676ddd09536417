"<div id=\"content\" class=\"body-content\">\n"+
"<h1 class=\"glossary\"><span class=\"CHAPTITLE\"><a name=\"CHAPTER39\" href='javascript:parent.parent.sC2(\"TOP\")' class=\"CHAPLINK\">Glossary</a></span></h1>\n" +
"<table class=\"GLOSSARY\"><tr><td class=\"term\" valign=\"top\"><b>Glossary</b></td></tr><tr><td class=\"expansion\">The OSS Glossary is available in <a href='javascript:parent.parent.parent.showAnchor(\"rf-OSSGlossary\")' class=\"xref\">Reference [1]</a>.</td></tr></table>\n" +
"<br />\n"+
"</div>\n";

var C40=
"<div id=\"content\" class=\"body-content\">\n"+
"<h1 class=\"reference\"><span class=\"CHAPTITLE\"><a name=\"CHAPTER40\" href='javascript:parent.parent.sC2(\"TOP\")' class=\"CHAPLINK\">Reference List</a></span></h1>\n" +
